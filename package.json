{
  "name": "nodejs-starter-project",
  "version": "1.0.0",
  "description": "Starter template for nodejs rest api project",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon --exec \"npm run build && node build/bundle.js\" -e js --ignore build/",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rimraf build && webpack --mode=development",
    "build:prod": "rimraf build && webpack",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "restapi"
  ],
  "author": "Subhadip Majumder",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@types/jest": "^29.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "haversine-distance": "^1.2.1",
    "imagekit": "^4.1.2",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.1.2",
    "sequelize": "^6.28.0",
    "stripe": "^11.14.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  }
}
